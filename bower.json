{
  "name": "painting",
  "private": true,
  "dependencies": {
    "modernizr": "^2",
    "vue": "^2.0.0",
    "vue-router": "^2.0.0",
    "leaflet": "^1.0.2",
    "es6-promise": "^3.0.2",
    "lodash": "*",
    "fetch": "*",
    "fontawesome": "~4.4.0",
    "pixi.js": "^4.3.0",
    "d3": "~4.2.2",
    "open-sans-fontface": "~1.4.2",
    "scrollmagic": "^2.0.5",
    "jssor-slider": "jssor#^21.1.5",
    "Vue2Leaflet": "SiggyF/Vue2Leaflet#91e01d679ff522773e786676e91fac6966d5acb9",
    "vuetify": "vuetifyjs/vuetify#51945ce6bfb8c20c478cdd8c705af1a5da18d85b",
    "material-design-icons": "^3.0.1",
    "roboto-fontface": "^0.6.0",
    "Leaflet.awesome-markers": "^2.0.2"
  },
  "overrides": {
    "pixi.js": {
      "old": "3.0.11",
      "main": [
        "dist/pixi.js"
      ]
    },
    "sidebar-v2": {
      "main": [
        "scss/leaflet-sidebar.scss",
        "js/jquery-sidebar.js",
        "js/leaflet-sidebar.js"
      ],
      "dependencies": {
        "leaflet": "^1.0.2"
      }
    },
    "bootstrap-sass": {
      "main": [
        "assets/stylesheets/_bootstrap.scss",
        "assets/fonts/bootstrap/*",
        "assets/javascripts/bootstrap.js"
      ]
    },
    "dat-gui": {
      "main": "build/dat.gui.js"
    },
    "open-sans-fontface": {
      "main": [
        "./open-sans.scss",
        "./fonts/**/*"
      ]
    },
    "scrollmagic": {
      "main": [
        "scrollmagic/uncompressed/ScrollMagic.js",
        "scrollmagic/uncompressed/plugins/debug.addIndicators.js"
      ]
    },
    "fontawesome": {
      "main": [
        "less/font-awesome.less",
        "scss/font-awesome.scss",
        "fonts/**/*"
      ]
    },
    "vue": {
      "main": [
        "dist/vue.js"
      ]
    },
    "Vue2Leaflet": {
      "main": [
        "dist/vue2-leaflet.js",
        "vue2-leaflet.js.map"
      ],
      "dependencies": {
        "leaflet": "^1.0.2"
      }
    },
    "vuetify": {
      "main": [
        "dist/vuetify.js",
        "dist/vuetify.min.css"
      ]
    },
    "material-design-icons": {
      "main": [
        "iconfont/material-icons.css",
        "iconfont/MaterialIcons-Regular.woff2",
        "iconfont/MaterialIcons-Regular.woff",
        "iconfont/MaterialIcons-Regular.ttf"
      ]
    },
    "roboto-fontface": {
      "main": [
        "css/roboto/roboto-fontface.css",
        "css/roboto-condensed/roboto-condensed-fontface.css",
        "fonts/**/*.woff2",
        "fonts/**/*.woff",
        "fonts/**/*.ttf"
      ]
    }
  },
  "devDependencies": {
    "chai": "~3.0.0",
    "mocha": "~2.2.5"
  }
}
